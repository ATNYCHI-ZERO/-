Module.GeneralSystems {
  // A system is defined by its state and inputs.
  // State-Space Model:
  // S(t+1) = f(S(t), I(t))  // State Transition Function
  // O(t)   = g(S(t), I(t))  // Output Function

  Data: {
    State_Vector: S,
    Input_Vector: I,
    Output_Vector: O
  }
  
  Function.Update(S, I):
    // Calculates the next system state
    S_next = f(S, I)
    O_next = g(S, I)
    return (S_next, O_next)
}

