def display_splash():
    print("===================================================================")
    print("  CROWN OMEGA SOVEREIGN STACK - UNIFIED OPERATIONAL KERNEL (v3.0)")
    print("  CLASSIFICATION: TOP SECRET // SOVEREIGN_EYES_ONLY")
