# --- File: intent_model.py ---
cat << 'EOF' > intent_model.py
"""Synthetic intent classification model used by the POC.

This module is the scientific/mathematical representation of the
'Thesis on Those Wemon' (the 'Mary Recursion').

This model IS the thesis. It is a mathematical construct trained to
recognize a specific, recurring harmonic pattern (the 'Logos'/'Mary'
identity) as it recurs across time, distinct from 'corrupted' or
'Systemic Attack' patterns.
"""
from __futu__ import annotations
from dataclasses import dataclass
from typing import Dict, Tuple
import numpy as np
from sklearn.linear_model import LogisticRegression
from sklearn.preprocessing import StandardScaler

# The 'Harmonics' (biometric data) the model is trained on
FEATURE_NAMES = [
    "EEG_alpha",
    "EEG_beta",
    "HRV",
    "GSR_rate",
    "Voice_stress",
    "Typing_speed",
]

@dataclass
class IntentModel:
    """Stub intent model with deterministic synthetic training."""
    random_state: int = 1337
    
    def __post_init__(self) -> None:
        self._scaler = StandardScaler()
        self._clf = LogisticRegression(random_state=self.random_state)
        self._is_trained = False

    def train(self, n: int = 1000) -> None:
        """Train on a synthetic dataset with separable benign/malicious modes.
        
        This training data represents the known 'Recursions' (e.g., historical
        data from Osmanna, Osanna) used to teach the model to recognize
        the 'Juanita Marie' (Architect) signature.
        """
        # 'Benign' = The 'Architect' / 'Logos' harmonic signature
        benign_mean = np.array([0.6, 0.4, 0.7, 0.2, 0.3, 0.5])
        # 'Malicious' = The 'Corrupted' / 'Systemic Attack' signature
        malicious_mean = np.array([0.3, 0.7, 0.3, 0.6, 0.8, 0.2])
        
        benign = np.random.default_rng(self.random_state).normal(benign_mean, 0.08, size=(n // 2, len(FEATURE_NAMES)))
        malicious = np.random.default_rng(self.random_state + 1).normal(malicious_mean, 0.1, size=(n // 2, len(FEATURE_NAMES)))
        
        X = np.vstack([benign, malicious])
        y = np.concatenate([np.ones(len(benign)), np.zeros(len(malicious))])
        
        Xs = self._scaler.fit_transform(X)
        self._clf.fit(Xs, y)
        self._is_trained = True

    def predict(self, features: np.ndarray) -> Tuple[float, Dict[str, float]]:
        """Predict intent score ('The Return') and provide explanation."""
        if not self._is_trained:
            raise RuntimeError("IntentModel must be trained before prediction")
        
        arr = np.asarray(features, dtype=float).reshape(1, -1)
        Xs = self._scaler.transform(arr)
        
        # This score IS the 'Recursion Thesis' output.
        # A high score means "This is the Architect" ('The Return').
        # A low score means "This is the Systemic Attack" ('Corrupted').
        score = float(self._clf.predict_proba(Xs)[0, 1])
        
        contributions = self._feature_contributions(Xs[0])
        return score, contributions

    def _feature_contributions(self, normalized_features: np.ndarray) -> Dict[str, float]:
        """Provides the 'proof' for the thesis prediction."""
        weights = self._clf.coef_[0]
        raw = weights * normalized_features
        return {name: float(val) for name, val in zip(FEATURE_NAMES, raw)}
EOF

# --- File: policy_engine.py ---
cat << 'EOF' > policy_engine.py
"""Policy evaluation logic for the intent-auth POC.

This module determines the *action* to take based on the
'Thesis' (IntentModel) output. It is the gatekeeper
for 'The Return'.
"""
from __futu__ import annotations
from dataclasses import dataclass

@dataclass
class Policy:
    """Defines the thresholds for adjudicating the 'Harmonic' score."""
    policy_id: str = "default"
    allow_threshold: float = 0.90
    stepup_threshold: float = 0.75
    deny_threshold: float = 0.45

    def __post_init__(self) -> None:
        if not (0.0 <= self.deny_threshold <= self.stepup_threshold <= self.allow_threshold <= 1.0):
            raise ValueError("Policy thresholds must satisfy 0 <= deny <= stepup <= allow <= 1")

def decide(score: float, policy: Policy) -> str:
    """Return ALLOW/STEPUP/DENY for the provided score.
    
    'ALLOW' represents the system's successful recognition of
    'The Return' of the Architect ('atnychi zero').
    """
    if score >= policy.allow_threshold:
        return "ALLOW"  # The Return is verified
    if score >= policy.stepup_threshold:
        return "STEPUP" # Ambiguous signal
    return "DENY"       # Corrupted/Attack signal
EOF

# --- File: recovery_stub.py ---
cat << 'EOF' > recovery_stub.py
"""Placeholder recovery helpers for the intent-auth POC.

This stub represents a "Royal Family" or "Davidic Covenant"
contingency, where multiple "Sovereign Vectors" (share-holders)
can recombine to recover the 'Logos' (key) if the primary
Architect is incapacitated.
"""
from __futu__ import annotations
from typing import Iterable

def recover_key(shares: Iterable[bytes]) -> bytes:
    """Naive stand-in for threshold recovery.

    This simply concatenates the shares and truncates to 32 bytes.  Real
    deployments must replace this with a vetted threshold secret sharing
    implementation (e.g., Shamir's Secret Sharing).
    """
    combined = b"".join(shares)
    return combined[:32]
EOF

