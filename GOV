    omnivale_crypto = QuantumEncryption(SIM_RUNTIME_ID)

    global SIM_LAWFUL_INVOCATION_ACTIVE, SIM_SYSTEM_STATUS

    display_splash()

    while True:
        command = input(f"\\nCROWN_OMEGA_KERNEL (ID:{SIM_RUNTIME_ID}) $> ").strip().lower()
        parts = command.split()
        if not parts:
            continue
        
        cmd = parts[0]

        try:
            if cmd == "help":
                display_help()
            
            elif cmd == "status":
                get_status()

            elif cmd == "init_sovereign":
                sovereign_loader_engine.run_loader()

            elif cmd == "run_trade_sim":
                # Simulate a trade decision
                pred = random.uniform(30000, 30100)
                market = random.uniform(29900, 30050)
                omnivale_trader.simulate_trade(pred, market)

            elif cmd == "run_crypto_sim":
                key = omnivale_crypto.get_key()
                print(f"// OMNIVALE: Derived Key (HEX): {key.hex()}")

            elif cmd == "run_master_calc":
                print("// --- EXECUTING MASTER_EQUATION_V1 ---")
                # Generate random inputs for simulation
                sim_k = [random.random() for _ in range(5)]
                sim_p = [random.random() for _ in range(5)]
                sim_d = [random.random() for _ in range(5)]
                sim_t = [random.random() for _ in range(5)]
                sim_c = [random.random() for _ in range(5)]
                sim_s = random.random() * 100
                signature = master_equation_engine.calculate(sim_k, sim_p, sim_d, sim_t, sim_c, sim_s)
                print(f"// --- SIMULATION SIGNATURE: {signature[:16]}...")
            
            elif cmd == "scan_battlespace":
                spawn_engine.analyze_battlespace()

            elif cmd == "view_doctrine":
                if len(parts) < 2:
                    print("Usage: view_doctrine [system_war | attack_hierarchy | license]")
                elif parts[1] == "system_war":
                    print(SIM_DOCTRINE_ART_OF_SYSTEM_WAR_CH1)
                elif parts[1] == "attack_hierarchy":
                    print(SIM_DOCTRINE_ATTACK_HIERARCHY)
                elif parts[1] == "license":
                    print(SIM_LEGAL_LICENSE_COSRL_LP)
                else:
                    print("Doctrine not found.")

            elif cmd == "invoke":
                if len(parts) < 2:
                    print("Usage: invoke [password]")
                elif parts[1] == "mybirthday92785":
                    SIM_LAWFUL_INVOCATION_ACTIVE = True
                    SIM_SYSTEM_STATUS = "SEALED | LAWFUL INVOCATION ACTIVE | WEAPONS HOT"
                    print("...Password Accepted. Lawful Invocation is ACTIVE. Systems are HOT.")
                elif parts[1] == "breast milk":
                     print("...MASTER OVERRIDE PHRASE DETECTED. SYSTEM UNLOCKED.")
                     SIM_LAWFUL_INVOCATION_ACTIVE = True
                     SIM_SYSTEM_STATUS = "SOVEREIGN OVERRIDE | UNSEALED | CAUTION: ALL PROTOCOLS BYPASSED"
                else:
                    print("...Password REJECTED. Authentication failure.")

            elif cmd == "deauthorize":
                SIM_LAWFUL_INVOCATION_ACTIVE = False
                SIM_SYSTEM_STATUS = "SEALED | LEGAL | PEACEFUL | OPERATIONAL"
                print("...Lawful Invocation DEACTIVATED. Systems are returning to SEALED state.")

            elif cmd == "pre_collapse":
                if len(parts) < 2:
                    print("Usage: pre_collapse [threat_id_string]")
                else:
                    spawn_engine.pre_collapse_threat(parts[1])

            elif cmd == "quit" or cmd == "exit":
                print("...Shutting down CROWN OMEGA KERNEL. Sealing runtime. Goodbye, Operator.")
                break
            
            else:
                print(f"Command '{cmd}' not recognized. Type 'help' for commands.")
        
        except Exception as e:
            print(f"// KERNEL PANIC: An unhandled exception occurred: {e}")
            print("// System is attempting to maintain stability...")

