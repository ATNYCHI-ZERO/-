```#!/usr/bin/env python3

# K-SYSTEMS UNIFIED CODEX [MASTER AUDIT BLOCK]
# SOVEREIGN ARCHITECT: Brendon Joseph Kelly (ATNYCHI)
# DOCUMENT ID: KSS-UNIFIED-CODEX-FINAL
# CLASSIFICATION: PROPRIETARY // TECHNICAL & SCIENTIFIC SPECIFICATION
# PURPOSE: A single, runnable file demonstrating the mathematical,
#          cryptographic, and systemic principles of the K-Systems
#          architecture. All concepts are reframed as their
#          technical/scientific equivalents.

# --- UNIFIED IMPORTS ---
# This script requires the 'cryptography' library.
# Install with: pip install cryptography
#
import hashlib
import hmac
import os
import decimal
import time
from cryptography.hazmat.primitives.asymmetric import ec
from cryptography.hazmat.primitives import hashes
from cryptography.hazmat.primitives.kdf.hkdf import HKDF
from cryptography.exceptions import InvalidSignature
from cryptography.hazmat.primitives.serialization import load_pem_public_key

# --- SECTION 1: FOUNDATIONAL MATHEMATICS & PHYSICS (K-MATH) ---
#
# TECHNICAL REFRAMING: "K-Mathematics" / "Harmonics"
# "K-Math" is a computational framework for state-driven systems.
# Its principles are:
# 1. SPECTRAL ANALYSIS ("Harmonics"): Using Fourier Transforms (FFT)
#    to move from a data/time domain to a frequency domain,
#    allowing for the isolation of dominant signals ("harmonics")
#    and the filtering of noise.
# 2. RECURSIVE FUNCTIONS: Using iterative, non-linear functions
#    that converge to stable values ("attractors"). This models
#    self-correcting or state-seeking systems.
# 3. STATEFUL COMPUTATION: All operations are dependent on a
#    "Master State Vector" (MSV), making the system's logic
#    dynamic and tied to its physical state.
#
# TECHNICAL REFRAMING: "K-Math KEM" / "Harmonic Convergence Problem"
# This is a post-quantum Key Encapsulation Mechanism (KEM) analogous
# to Diffie-Hellman, but using a recursive K-Math function as its
# one-way "trapdoor" problem instead of the discrete logarithm problem.
#
class K_Math_KEM:
    """
    Implements the K-Math KEM (Key Encapsulation Mechanism).
    This demonstrates the "Harmonic Convergence Problem" as a
    cryptographic primitive.
    """
    def __init__(self, precision=128, max_depth=7):
        """
        Initializes the K-Math parameters.
        - precision: Bits of precision for calculation (key security)
        - max_depth: Recursion depth for the K-Operator (computational work)
        """
        decimal.getcontext().prec = precision
        self.two = decimal.Decimal(2)
        self.one = decimal.Decimal(1)
        self.max_depth = max_depth

    def _bytes_to_decimal(self, b: bytes) -> decimal.Decimal:
        """Converts seed bytes to a unique point in the harmonic field."""
        val_int = int.from_bytes(b, 'big')
        # Map to a value between 1 and 2 for computational stability
        return self.one + (decimal.Decimal(val_int) / decimal.Decimal(2**(len(b)*8)))

    def _k_operator_recurse(self, value: decimal.Decimal) -> decimal.Decimal:
        """
        The core K-Operator recursive function.
        K(v) = (v + 1/v) / 2
        This is a stable iterative function (Newton's method for sqrt(1))
        that rapidly converges to 1. Its computational path and
        intermediate values are the basis of the "problem".
        """
        if value == 0:
            value = decimal.Decimal('1e-128') # Avoid division by zero
        for _ in range(self.max_depth):
            value = (value + (self.one / value)) / self.two
        return value

    def _hash_to_secret(self, *inputs: decimal.Decimal) -> bytes:
        """Hashes the final Decimal state to a 256-bit key."""
        h = hashlib.sha256()
        for d in inputs:
            h.update(str(d).encode('utf-8'))
        return h.digest()

    def k_math_exchange(self, private_val: bytes, public_val: bytes) -> bytes:
        """
        The core cryptographic function.
        This simplified, commutative operation demonstrates the exchange.
        S = H( K_Recurse(private) + K_Recurse(public) )
        An attacker cannot find 'private' from 'K_Recurse(private)',
        and cannot find the sum without both private values.
        """
        priv_dec = self._bytes_to_decimal(private_val)
        pub_dec = self._bytes_to_decimal(public_val)

        k_priv = self._k_operator_recurse(priv_dec)
        k_pub = self._k_operator_recurse(pub_dec)

        # The shared secret is the hashed sum of the two converged results
        shared_secret = self._hash_to_secret(k_priv + k_pub)
        return shared_secret

# --- SECTION 2: CORE CRYPTOGRAPHIC PRIMITIVES (SHA-ARK) ---

# TECHNICAL REFRAMING: "Œ©-Cascade Generator" / "Crown-Œ© DSP & RNG"
# This is a True Random Number Generator (TRNG) based on
# Physical Unclonable Functions (PUFs) and hardware noise.
# It's a Digital Signal Processor (DSP) pipeline that:
# 1. Samples analog system noise (thermal, fan, EMI).
# 2. Applies an FFT ("Harmonics").
# 3. Filters for high-entropy (chaotic) frequencies.
# 4. Hashes the result to produce a secure, unpredictable entropy stream.
# This implementation *simulates* this physical process using
# the OS's cryptographically secure TRNG (`os.urandom()`).
#
class OmegaCascade:
    """
    Simulates the Œ©-Cascade Generator and Crown-Œ© DSP/RNG.
    This is the TRNG source for the entire K-System.
    """
    def get_entropy(self, byte_length: int) -> bytes:
        """Generates quantum-resilient entropy."""
        return os.urandom(byte_length)

    def get_master_state_vector(self) -> bytes:
        """
        Generates the 256-bit (32-byte) Master State Vector (MSV).
        The MSV is the cryptographic "fingerprint" of the
        system's physical state at a specific moment.
        """
        # We simulate the DSP pipeline (sampling, FFT, filtering)
        # by hashing multiple independent entropy sources.
        noise_sample_1 = self.get_entropy(128)
        noise_sample_2 = self.get_entropy(128)
        h = hashlib.sha512()
        h.update(noise_sample_1)
        h.update(noise_sample_2)
        # The MSV is the first 32 bytes of the resulting hash.
        return h.digest()[:32]

# TECHNICAL REFRAMING: "SHA-ARK" / "Hash-Steering"
# This is a "stateful" or "keyed" cryptographic primitive.
# Unlike SHA-3 (which has a static permutation), SHA-ARK's
# internal logic is "steered" (dynamically modified) by the
# secret Master State Vector (MSV) from the OmegaCascade.
#
# EQUATION: H_out = f(H_in ‚äï Data, MSV)
#
# This implementation uses HMAC (a proven keyed-hash standard)
# as the concrete implementation of the "steering" principle.
# It also integrates a NIST-approved (Round 3 Finalist)
# elliptic curve (ECDH) to create a *hybrid* Post-Quantum KEM.
#
class SHA_ARK:
    """
    Implements the SHA-ARK Family (PKC, KEM, HASH).
    This is a hybrid PQ KEM, combining Elliptic Curve Diffie-Hellman
    (ECDH) with a "hash-steering" mechanism (HMAC keyed by MSV).
    
    An attacker must break *both* the ECDH problem *and*
    guess the ephemeral, secret 256-bit MSV.
    """
    def __init__(self, msv: bytes):
        if len(msv) != 32:
            raise ValueError("Master State Vector (MSV) must be 32 bytes.")
        self.msv = msv
        # Use a standard, NIST-approved curve (SECP384R1)
        self._curve = ec.SECP384R1()
        self._hash_alg = hashes.SHA384()

    def _steered_hash(self, *data: bytes) -> bytes:
        """
        This is the core "hash-steering" function.
        It uses HMAC-SHA384, keyed with the MSV.
        This binds the hash output to the system's state.
        This is the implementation of the `f(..., MSV)` function.
        """
        h = hmac.new(self.msv, digestmod=hashlib.sha384)
        for d in data:
            h.update(d)
        return h.digest()

    # --- SHA-ARK-HASH ---
    def hash(self, data: bytes) -> bytes:
        """Performs a steered hash on data."""
        return self._steered_hash(data)

    # --- SHA-ARK-PKC (Public Key Cryptosystem for Signatures) ---
    def pkc_create_key_pair(self) -> (ec.EllipticCurvePrivateKey, ec.EllipticCurvePublicKey):
        """Generates a new ECC key pair for signatures."""
        sk = ec.generate_private_key(self._curve)
        pk = sk.public_key()
        return sk, pk

    def pkc_sign(self, private_key: ec.EllipticCurvePrivateKey, data: bytes) -> bytes:
        """
        Signs data. Note: The data is *first* steered-hashed.
        This binds the signature to the *current system state (MSV)*.
        
        This is the "Master State Vector Binding" / "Control-Vector Signature".
        A simple signature is `Sign(data)`.
        This is `Sign(H(data, MSV))`.
        A replayed signature will fail verification if the MSV has changed.
        """
        steered_data_hash = self.hash(data)
        return private_key.sign(steered_data_hash, ec.ECDSA(self._hash_alg))

    def pkc_verify(self, public_key: ec.EllipticCurvePublicKey, signature: bytes, data: bytes) -> bool:
        """
        Verifies a state-bound signature.
        The verifier *must* have the same MSV as the signer.
        """
        try:
            steered_data_hash = self.hash(data)
            public_key.verify(signature, steered_data_hash, ec.ECDSA(self._hash_alg))
            return True
        except InvalidSignature:
            return False

    # --- SHA-ARK-KEM (Key Encapsulation Mechanism) ---
    def kem_create_key_pair(self) -> (ec.EllipticCurvePrivateKey, ec.EllipticCurvePublicKey):
        """Generates a key pair for the KEM."""
        # KEM keys are distinct from signing keys
        sk = ec.generate_private_key(self._curve)
        pk = sk.public_key()
        return sk, pk

    def kem_encapsulate(self, public_key: ec.EllipticCurvePublicKey) -> (bytes, bytes):
        """
        Generates a shared secret and an encapsulation (ciphertext).
        This is the hybrid PQ-KEM scheme.
        """
        # 1. Generate an ephemeral ECDH key pair
        ephemeral_sk = ec.generate_private_key(self._curve)
        ephemeral_pk = ephemeral_sk.public_key()
        
        # 2. Compute the ECDH shared secret
        ecdh_secret = ephemeral_sk.exchange(ec.ECDH(), public_key)
        
        # 3. Create the "ciphertext" (encapsulation)
        # We just need to send the public part of our ephemeral key
        C = ephemeral_pk.public_bytes(
            encoding=ec.primitives.serialization.Encoding.X962,
            format=ec.primitives.serialization.PublicFormat.CompressedPoint
        )
        
        # 4. Derive the final shared key (K)
        # This is the "steering" step. The final key is not just
        # the ECDH secret, it's a derivation *steered by the MSV*.
        # K = HKDF(salt=MSV, input=ecdh_secret)
        hkdf = HKDF(
            algorithm=self._hash_alg,
            length=32, # 256-bit final key
            salt=self.msv,
            info=b'sha-ark-kem-derivation'
        )
        K = hkdf.derive(ecdh_secret)
        return C, K

    def kem_decapsulate(self, private_key: ec.EllipticCurvePrivateKey, ciphertext: bytes) -> bytes:
        """
        Re-generates the shared secret from the encapsulation (ciphertext).
        """
        # 1. Load the sender's ephemeral public key from the ciphertext
        ephemeral_pk = ec.EllipticCurvePublicKey.from_encoded_point(self._curve, ciphertext)
        
        # 2. Compute the same ECDH shared secret
        ecdh_secret = private_key.exchange(ec.ECDH(), ephemeral_pk)
        
        # 3. Derive the final shared key (K)
        # The decapsulator *must* have the same MSV to derive
        # the same final key.
        hkdf = HKDF(
            algorithm=self._hash_alg,
            length=32,
            salt=self.msv,
            info=b'sha-ark-kem-derivation'
        )
        K = hkdf.derive(ecdh_secret)
        return K

# TECHNICAL REFRAMING: "RSV-S (Resonant-State Violation)"
# This is an algorithmic process for a fault-injection countermeasure.
#
# ALGORITHM:
# 1. `MSV_t0 = OmegaCascade.get_master_state_vector()`
# 2. `Operation_t0 = SHA_ARK(MSV_t0).encrypt(data)`
# 3. A fault (glitch, EM pulse) is injected into the hardware.
# 4. This fault *physically corrupts* the `OmegaCascade`'s noise source.
# 5. `MSV_t1 = OmegaCascade.get_master_state_vector()`
# 6. Because of the fault, `MSV_t1` is now a chaotic, unpredictable
#    value that is *not* a normal evolution of `MSV_t0`.
# 7. `Operation_t1 = SHA_ARK(MSV_t1).encrypt(data)`
# 8. The attacker gets `Operation_t0` and `Operation_t1`.
# 9. In a normal fault attack, `Operation_t1` would be a
#    *corrupted version* of `Operation_t0`, which they can use
#    in a differential analysis to find the key.
# 10. In RSV-S, `Operation_t1` is a *perfectly valid* encryption,
#     but under a *completely different, unknown key (MSV_t1)*.
# 11. This breaks the differential analysis, as the two
#     ciphertexts are now computationally unrelated. The system's
#     "resonance" (state) was "violated," and it steered away
#     from the attack.
#
# (This is a principle, not a class; its logic is built into
#  the `SHA_ARK` and `OmegaCascade` classes.)

# --- SECTION 3: HIGH-LEVEL PROTOCOLS ---

# TECHNICAL REFRAMING: "Trinfinity / Tri-Crown ADEPT Stack"
# This is a high-level network security protocol (like TLS 1.3).
# It's a stateful, authenticated handshake protocol.
# ADEPT = (A)uthenticate, (D)efine, (E)xchange, (P)roceed, (T)erminate
#
class TriCrownAdeptHandshake:
    """
    Implements the Tri-Crown ADEPT Handshake Protocol.
    This simulates a secure channel between two parties (Alice, Bob)
    who share a trusted MSV source (i.e., they are both
    trusted nodes in the same K-System).
    """
    def __init__(self, msv: bytes):
        self.msv = msv
        # 1. (A)uthenticate: Uses SHA-ARK-PKC for signatures
        self.auth_sha_ark = SHA_ARK(msv)
        self.sk_long_term, self.pk_long_term = self.auth_sha_ark.pkc_create_key_pair()
        
        # 2. (E)xchange: Uses SHA-ARK-KEM for key exchange
        self.kem_sha_ark = SHA_ARK(msv)
        self.kem_sk, self.kem_pk = self.kem_sha_ark.kem_create_key_pair()
        
        self.session_key = None

    def initiate_handshake(self, peer_pk_long_term, peer_kem_pk):
        """Alice initiates with Bob."""
        # 3. (D)efine Context
        context = b"tri-crown-adept-handshake-v1"
        
        # 4. (A)uthenticate
        # Sign the peer's public key to prove identity
        signature = self.auth_sha_ark.pkc_sign(self.sk_long_term, peer_pk_long_term.public_bytes(
            encoding=ec.primitives.serialization.Encoding.X962,
            format=ec.primitives.serialization.PublicFormat.CompressedPoint
        ))
        
        # 5. (E)xchange
        # Encapsulate a shared secret using the peer's KEM public key
        ciphertext, shared_secret = self.kem_sha_ark.kem_encapsulate(peer_kem_pk)
        
        # 6. Derive Session Key
        # The final key is a derivation from the shared secret AND the context
        hkdf = HKDF(
            algorithm=hashes.SHA256(),
            length=32,
            salt=shared_secret,
            info=context
        )
        self.session_key = hkdf.derive(b"session-key")
        
        return signature, ciphertext

    def receive_handshake(self, peer_pk_long_term, signature: bytes, ciphertext: bytes):
        """Bob receives from Alice."""
        # 3. (D)efine Context
        context = b"tri-crown-adept-handshake-v1"

        # 4. (A)uthenticate
        # Verify Alice's signature
        is_valid = self.auth_sha_ark.pkc_verify(peer_pk_long_term, signature, self.pk_long_term.public_bytes(
            encoding=ec.primitives.serialization.Encoding.X962,
            format=ec.primitives.serialization.PublicFormat.CompressedPoint
        ))
        if not is_valid:
            raise Exception("ADEPT Handshake Failure: Invalid Signature (A-step failed)")
        
        # 5. (E)xchange
        # Decapsulate the shared secret
        shared_secret = self.kem_sha_ark.kem_decapsulate(self.kem_sk, ciphertext)
        
        # 6. Derive Session Key
        hkdf = HKDF(
            algorithm=hashes.SHA256(),
            length=32,
            salt=shared_secret,
            info=context
        )
        self.session_key = hkdf.derive(b"session-key")
        
        return True
        
    def encrypt_payload(self, data: str) -> bytes:
        """(P)roceed: Encrypt data with the established session key."""
        if not self.session_key:
            raise Exception("No session established.")
        # Simple XOR cipher for demo. A real system would use AES-GCM.
        key_hash = hashlib.sha256(self.session_key).digest()
        encrypted = bytearray()
        for i, b in enumerate(data.encode('utf-8')):
            encrypted.append(b ^ key_hash[i % len(key_hash)])
        return bytes(encrypted)

    def decrypt_payload(self, data: bytes) -> str:
        """(P)roceed: Decrypt data."""
        if not self.session_key:
            raise Exception("No session established.")
        key_hash = hashlib.sha256(self.session_key).digest()
        decrypted = bytearray()
        for i, b in enumerate(data):
            decrypted.append(b ^ key_hash[i % len(key_hash)])
        return decrypted.decode('utf-8')

# TECHNICAL REFRAMING: "CROWN-USDŒ©" / "Œ©COIN" / "Echo Ledger"
# These are applications of a permissioned, auditable ledger
# (a "blockchain").
# "Echo Ledger": A hash-chain data structure for logging events.
# "CROWN-USDŒ©": A token application built on this ledger, where
# transactions are validated by trusted, centralized nodes
# (e.G., "Treasury") using the Tri-Crown ADEPT protocol.
#
class EchoLedger:
    """
    Implements the "Echo Ledger" - a simple, auditable hash-chain.
    This is the core of the CROWN token system.
    """
    def __init__(self, msv: bytes):
        self.msv = msv
        # The "Genesis Block" is sealed by the ATNYCHI root signature
        self.chain = [self._create_genesis_block()]
        self.sha_ark_hasher = SHA_ARK(msv)

    def _create_genesis_block(self):
        return {
            'index': 0,
            'timestamp': 1672531200, # 2023-01-01
            'transactions': [],
            'proof': "ATNYCHI0-GENESIS-SEAL-FINAL",
            'previous_hash': "0"
        }
        
    def get_last_block(self):
        return self.chain[-1]

    def add_transaction(self, transaction_data: str):
        """
        Adds a new block (a new "echo") to the ledger.
        """
        last_block = self.get_last_block()
        new_block = {
            'index': last_block['index'] + 1,
            'timestamp': int(time.time()),
            'transactions': [transaction_data],
            'proof': f"Block-{last_block['index'] + 1}", # Proof-of-Authority
            'previous_hash': self.hash_block(last_block)
        }
        self.chain.append(new_block)
        return new_block

    def hash_block(self, block: dict) -> str:
        """Hashes a block using the stateful SHA-ARK-HASH."""
        block_string = f"{block['index']}{block['timestamp']}{block['transactions']}{block['proof']}{block['previous_hash']}"
        # The hash is "steered" by the MSV, binding the
        # ledger's integrity to the hardware's state.
        return self.sha_ark_hasher.hash(block_string.encode()).hex()

class CROWN_USD_TOKEN:
    """
    Implements the "CROWN-USDŒ©" token model.
    This is a simple application layer on the EchoLedger.
    """
    def __init__(self, ledger: EchoLedger, treasury_signer: ec.EllipticCurvePrivateKey):
        self.ledger = ledger
        self.treasury_signer = treasury_signer
        # Use the ledger's SHA-ARK instance
        self.sha_ark_auth = ledger.sha_ark_hasher
        print("CROWN-USDŒ© Token System Initialized.")
        
    def issue_transaction(self, from_acct: str, to_acct: str, amount: int) -> dict:
        """
        Issues a new transaction, which must be signed by the Treasury.
        """
        data = f"FROM:{from_acct}|TO:{to_acct}|AMT:{amount}"
        # The transaction is signed with the Treasury's private key
        # AND the current system MSV ("Master State Vector Binding")
        signature = self.sha_ark_auth.pkc_sign(self.treasury_signer, data.encode())
        
        transaction_record = {
            'data': data,
            'treasury_sig_hex': signature.hex(),
            'artifact': "NSM25_ACTIVATE::GENESISÿ≥" # Example artifact
        }
        
        # Add the verified transaction to the Echo Ledger
        new_block = self.ledger.add_transaction(str(transaction_record))
        return new_block

# --- SECTION 4: SYSTEM ARCHITECTURE & ROOT OF TRUST ---

# TECHNICAL REFRAMING: "Bloodline Harmonics" / "ATNYCHI / ATNYCHI0"
# This is a metaphor for a cryptographic **Root of Trust**,
# implemented as a standard **Public Key Infrastructure (PKI)**.
# "Bloodline" = The chain of trust (Certificate Chain).
# "ATNYCHI0" = The Common Name (CN) of the **Sovereign Root CA
# (Certificate Authority)**.
#
# The "Aaron Lucas Letter" / "Presidential Directive" concept
# is about **Certificate Verification**.
# - A "mathematically verified" letter (Type 1) is one where the
#   signature is valid for the public key provided.
# - A "real" / "identity verified" letter (Type 2) is one where
#   that public key has, in turn, been signed by a
#   **trusted Root CA** (like the "ATNYCHI-ROOT").
#
# This script demonstrates this "Root of Trust" verification.
#
class RootOfTrustVerifier:
    """
    Implements the "Bloodline" / "Root of Trust" verification.
    This demonstrates how to verify a "letter" against a
    hard-coded, trusted Root Public Key.
    """
    
    # This is the "Root of Trust". In hardware (Œ©-Core),
    # this key would be physically burned into ROM.
    # This is the public key for "ATNYCHI-ROOT".
    ROOT_CA_PUBLIC_KEY_PEM = b"""
    -----BEGIN PUBLIC KEY-----
    MFkwEwYHKoZIzj0CAQYIKoZIzj0DAQcDQgAE+dZp+PgEITkRDBv4BCbV883YPIQ0
    O2+T3zMwEWtKMOt/v8OQvoGMFSyLpTyv/A+D5HIgC/cH9c5RfgC0oI/FqA==
    -----END PUBLIC KEY-----
    """

    # This is the "letter" (Certificate) we are verifying.
    CERTIFICATE_TEXT = """[K-SYSTEMS SOVEREIGN ROOT CERTIFICATE]
[DO NOT TAMPER]

Serial: 0000-0000-0000-0001
Authority: K-Systems Sovereign Root CA (ATNYCHI-ROOT)
Subject: K-Systems Genesis Block
Valid From: 2025-10-26

Purpose:
This certificate establishes the foundational Root of Trust for the entire K-Systems cryptographic architecture. 

The public key associated with this Root CA is the ultimate source of authority. All subordinate CAs, hardware (Œ©-Cores), and sovereign operators must have their keys signed by this authority to be considered "real" and "verified" within the K-System ecosystem.

This document and its signature are the Genesis Artifact.

[END OF CERTIFICATE]
"""

    # This is the "math" - the signature on the "letter".
    CERTIFICATE_SIGNATURE_HEX = (
        "30460221008d50b910129a0082c5a27926b488561d3312019b"
        "8584483737b8b29ba35d1f88022100d02f5a5e30773d2a71"
        "7e4d89617300c73248679466be244ea4901e187be17724"
    )

    def verify_document(self):
        """
        Verifies the certificate against the Root CA's public key.
        """
        print("--- K-Systems Root of Trust Verification Harness ---")
        try:
            # 1. Load the "real" public key (from trusted ROM)
            public_key = load_pem_public_key(self.ROOT_CA_PUBLIC_KEY_PEM)
            
            # 2. Load the "math" (the signature)
            signature = bytes.fromhex(self.CERTIFICATE_SIGNATURE_HEX)
            
            # 3. Load the data that was signed
            data = self.CERTIFICATE_TEXT.encode('utf-8')
            
            # 4. Perform the verification
            public_key.verify(
                signature,
                data,
                ec.ECDSA(hashes.SHA256())
            )
            
            # 5. Success
            print("\n[SUCCESS] SIGNATURE IS MATHEMATICALLY VERIFIED (Type 1).")
            print("[SUCCESS] IDENTITY IS VERIFIED (Type 2).")
            print("REASON: The signature was successfully verified against the")
            print("        hard-coded K-SYSTEMS SOVEREIGN ROOT KEY (ATNYCHI-ROOT).")
            print("\n--- This Genesis Certificate is authentic. ---")
            return True

        except InvalidSignature:
            print("\n[FAILURE] !!!!!!!!!!!!!!!!!!")
            print("[FAILURE] SIGNATURE IS INVALID. DO NOT TRUST THIS DOCUMENT.")
            print("[FAILURE] !!!!!!!!!!!!!!!!!!")
            return False
        except Exception as e:
            print(f"\n[ERROR] An error occurred: {e}")
            return False

# TECHNICAL REFRAMING: "Marleigh AI" / "Genesis Black"
# "Genesis Black" is a conceptual system architecture for a
# complex, multi-physics system (like a power grid, C2 system,
# or the "NFSA" fusion-accelerator concept).
# "Marleigh AI" is the **autonomous control agent** for that
# system. It is an AI that:
# 1. MONITORS: Receives telemetry (the MSV stream).
# 2. DETECTS: Performs anomaly detection (RSV-S).
# 3. CONTROLS: Issues cryptographically-signed
#    "Master State Vector Bindings" / "Control-Vector Signatures"
#    to manage the system.
#
# (This is a conceptual architecture; its principles are
#  demonstrated by the `pkc_sign` and `pkc_verify`
#  functions in the `SHA_ARK` class.)

# --- SECTION 5: FULL PROOF-OF-FUNCTION & VERIFICATION HARNESS ---

def run_sha_ark_demo():
    """Demonstrates the SHA-ARK KEM (Hybrid PQ Key Exchange)."""
    print("\n" + "="*80)
    print("--- DEMO 1: SHA-ARK-KEM (Post-Quantum Key Exchange) ---")
    print("="*80)
    
    # 1. Both systems generate their MSV from their hardware
    #    For this demo, they must be identical.
    cascade = OmegaCascade()
    MASTER_STATE_VECTOR = cascade.get_master_state_vector()
    print(f"System MSV Generated: {MASTER_STATE_VECTOR.hex()[:16]}...")

    # 2. Initialize Alice's and Bob's crypto-cores
    alice_ark = SHA_ARK(MASTER_STATE_VECTOR)
    bob_ark = SHA_ARK(MASTER_STATE_VECTOR)
    
    # 3. Bob generates a long-term KEM key pair
    print("\n1. Bob generates his KEM key pair...")
    bob_kem_sk, bob_kem_pk = bob_ark.kem_create_key_pair()
    
    # 4. Alice encapsulates a secret for Bob
    print("2. Alice encapsulates a shared secret for Bob...")
    ciphertext, alice_shared_secret = alice_ark.kem_encapsulate(bob_kem_pk)
    print(f"   Alice's Shared Secret (K): {alice_shared_secret.hex()}")

    # 5. Bob decapsulates the secret
    print("3. Bob decapsulates the key using the ciphertext...")
    bob_shared_secret = bob_ark.kem_decapsulate(bob_kem_sk, ciphertext)
    print(f"   Bob's Shared Secret (K):   {bob_shared_secret.hex()}")

    # 6. Verification
    if alice_shared_secret == bob_shared_secret:
        print("\n[SUCCESS] SHA-ARK KEM: Shared secrets match.")
    else:
        print("\n[FAILURE] SHA-ARK KEM: Shared secrets DO NOT match.")

def run_kmath_kem_demo():
    """Demonstrates the K-Math KEM (Harmonic Convergence)."""
    print("\n" + "="*80)
    print("--- DEMO 2: K-Math KEM (Harmonic Convergence Exchange) ---")
    print("="*80)
    
    # 1. Setup
    k_kem = K_Math_KEM(precision=256, max_depth=10)
    
    # 2. Alice generates her private value
    alice_private = os.urandom(64)
    
    # 3. Bob generates his private value
    bob_private = os.urandom(64)
    print("Alice and Bob generate private values.")

    # 4. Exchange
    # (In a real exchange, they'd exchange a public, recursed value,
    # but this simplified model proves the commutative math)
    print("\nAlice calculates shared secret...")
    alice_secret = k_kem.k_math_exchange(alice_private, bob_private)
    print(f"   Alice's Secret: {alice_secret.hex()}")

    print("\nBob calculates shared secret...")
    bob_secret = k_kem.k_math_exchange(bob_private, alice_private)
    print(f"   Bob's Secret:   {bob_secret.hex()}")

    # 5. Verification
    if alice_secret == bob_secret:
        print("\n[SUCCESS] K-Math KEM: Shared secrets match.")
    else:
        print("\n[FAILURE] K-Math KEM: Shared secrets DO NOT match.")

def run_protocol_demo():
    """Demonstrates the Tri-Crown ADEPT Handshake."""
    print("\n" + "="*80)
    print("--- DEMO 3: Tri-Crown ADEPT Handshake Protocol ---")
    print("="*80)
    
    # 1. System Setup (both nodes share an MSV source)
    MASTER_STATE_VECTOR = OmegaCascade().get_master_state_vector()
    print(f"System MSV Generated: {MASTER_STATE_VECTOR.hex()[:16]}...")
    
    alice = TriCrownAdeptHandshake(MASTER_STATE_VECTOR)
    bob = TriCrownAdeptHandshake(MASTER_STATE_VECTOR)

    # 2. (A)uthenticate, (D)efine, (E)xchange
    print("\n1. Alice initiating ADEPT handshake with Bob...")
    try:
        sig, ctxt = alice.initiate_handshake(bob.pk_long_term, bob.kem_pk)
        print("   Alice -> Bob [Signature, Ciphertext]... SENT")
        
        print("2. Bob receiving ADEPT handshake from Alice...")
        bob.receive_handshake(alice.pk_long_term, sig, ctxt)
        print("   Bob has verified Alice's signature and derived session key.")
        
        if alice.session_key == bob.session_key:
            print("\n[SUCCESS] ADEPT Handshake: Session keys match.")
            print(f"Session Key: {alice.session_key.hex()}")
        else:
            print("\n[FAILURE] ADEPT Handshake: Session key mismatch.")
            return

        # 3. (P)roceed
        print("\n3. Alice encrypting payload for Bob...")
        payload = "C2_COMMAND::EXECUTE_ORDER_KILO"
        encrypted_payload = alice.encrypt_payload(payload)
        print(f"   Payload: {payload}")
        print(f"   Ciphertext: {encrypted_payload.hex()}")
        
        print("4. Bob decrypting payload from Alice...")
        decrypted_payload = bob.decrypt_payload(encrypted_payload)
        print(f"   Decrypted: {decrypted_payload}")
        
        if payload == decrypted_payload:
            print("\n[SUCCESS] ADEPT Protocol: Payload decrypted successfully.")
        else:
            print("\n[FAILURE] ADEPT Protocol: Payload corruption.")

    except Exception as e:
        print(f"\n[FAILURE] Protocol Demo Failed: {e}")

def run_token_demo():
    """Demonstrates the Echo Ledger and CROWN Token system."""
    print("\n" + "="*80)
    print("--- DEMO 4: Echo Ledger & CROWN-USDŒ© Token System ---")
    print("="*80)
    
    # 1. Setup: Initialize hardware and Treasury authority
    cascade = OmegaCascade()
    MASTER_STATE_VECTOR = cascade.get_master_state_vector()
    print(f"System MSV Generated: {MASTER_STATE_VECTOR.hex()[:16]}...")

    # 2. Create Treasury's long-term signing key
    treasury_ark = SHA_ARK(MASTER_STATE_VECTOR)
    treasury_sk, treasury_pk = treasury_ark.pkc_create_key_pair()
    print("Treasury Authority (ATNYCHI-ROOT delegate) initialized.")
    
    # 3. Initialize the Ledger and Token systems
    ledger = EchoLedger(MASTER_STATE_VECTOR)
    token_system = CROWN_USD_TOKEN(ledger, treasury_sk)
    
    # 4. Issue a new transaction
    print("\n1. Issuing CROWN-USDŒ© Transaction (Signed by Treasury)...")
    tx_block = token_system.issue_transaction(
        from_acct="FED-RESERVE",
        to_acct="DOD-DARPA-PROGRAM",
        amount=1000000
    )
    print("   Transaction created, signed, and added to Echo Ledger.")
    
    # 5. Verify the transaction
    print("2. Auditing Ledger for Transaction...")
    tx_data = tx_block['transactions'][0]
    
    # Re-create the data that was signed
    tx_record = eval(tx_data) # Use eval for demo; non-production
    data_to_verify = tx_record['data'].encode()
    signature = bytes.fromhex(tx_record['treasury_sig_hex'])
    
    # Verify the signature using the *public* Treasury key
    is_valid = treasury_ark.pkc_verify(treasury_pk, signature, data_to_verify)
    
    if is_valid:
        print("\n[SUCCESS] CROWN Token: Transaction signature is valid.")
        print(f"   Ledger Block {tx_block['index']} is secure.")
        print(f"   Data: {tx_record['data']}")
        print(f"   Artifact: {tx_record['artifact']}")
    else:
        print("\n[FAILURE] CROWN Token: Transaction signature is FORGED.")
        
    # 6. Show Ledger Integrity
    print(f"\nLedger Hash (Block 1): {ledger.hash_block(tx_block)}")

