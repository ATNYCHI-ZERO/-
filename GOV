    "iat": 1730000000,   // Issued At (Timestamp)
    "aud": "GOVT-REVIEW", // Audience
    "primitives": [      // Chain of primitives
      {
        "operator": "ML-KEM",
        "params": {"n": 768, "q": 3329}
      },
      {
        "operator": "McEliece",
        "params": {"m": 12, "n": 6960, "t": 119}
      }
    ],
