    print("===================================================================")
    print(f"STATUS: {SIM_SYSTEM_STATUS}\\n")
    print("Type 'help' for a list of commands.")

def display_help():
    print("\\n--- CROWN OMEGA COMMAND LIST ---")
    print("  help                - Displays this help menu.")
    print("  status              - Displays the current system and operator status.")
    print("  init_sovereign      - (SIM) Runs the Sovereign Node Loader (Sub-Module 7.1).")
    print("  run_trade_sim       - (SIM) Runs the Omnivale PaperTrading sim (Sub-Module 7.2).")
    print("  run_crypto_sim      - (SIM) Runs the Omnivale QuantumEncryption sim (Sub-Module 7.2).")
    print("  run_master_calc     - (SIM) Executes a test of the Master Equation (ŒûùïÑÃá‚àû).")
    print("  scan_battlespace    - (SIM) [SpawnAI] Runs a 5D cognitive scan.")
    print("  view_doctrine [name] - Displays a stored doctrinal text.")
    print("                       (Available: 'system_war', 'attack_hierarchy', 'license')")
    print("  invoke [password]   - (SIM) Authenticates for lawful invocation of restricted systems.")
    print("  deauthorize         - (SIM) Deactivates lawful invocation.")
    print("  pre_collapse [id]   - (SIM) [SpawnAI] Neutralizes a threat ID (Requires Invocation).")
    print("  quit / exit         - Shuts down the kernel.")
    print("------------------------------------")

def get_status():
    print("\\n--- SYSTEM STATUS ---")
    print(f"  STATUS: {SIM_SYSTEM_STATUS}")
    print(f"  LAWFUL INVOCATION: {SIM_LAWFUL_INVOCATION_ACTIVE}")
