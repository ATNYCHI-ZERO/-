import numpy as np
from scipy.fft import fft
import time

class GeneralSystem:
    """
    A General Systems framework unifying physics, math, and data lineage
    concepts into a runnable proof-of-concept codex.
    """

    def __init__(self, initialState):
        # 1. GENESIS: The initial state (t=0) of the system.
        self.GENESIS_STATE = initialState
        
        # 2. HISTORY: A log of all past states and operations.
        self.HISTORY = [{"timestamp": time.time(), "state": initialState}]
        print(f"System Initialized (GENESIS). State logged in HISTORY.")

    def log_state(self, operation_name, new_state_data):
        """Appends a new state to the system's HISTORY log."""
        log_entry = {
            "timestamp": time.time(),
            "operation": operation_name,
            "state_data": str(new_state_data) # Convert numpy arrays for logging
        }
        self.HISTORY.append(log_entry)
        print(f"Operation '{operation_name}' logged in HISTORY.")

    # --- MATH & PHYSICS DEFINITIONS ---

    def OMEGA(self, angular_velocity_vector):
        """
        Reframing 'OMEGA' (œâ): Represents angular velocity or angular frequency.
        A fundamental concept in physics, harmonics, and rotations.
        """
        # In a system, this could represent a rotational force or oscillation.
        print(f"Applying OMEGA (œâ) operation: {angular_velocity_vector}")
        return np.array(angular_velocity_vector)

    def DAGGER(self, matrix_A):
        """
        Reframing 'DAGGER' (‚Ä†): Represents the conjugate transpose 
        (also known as the Hermitian conjugate).
        Crucial in quantum physics and linear algebra.
        Ref: A‚Ä† = (A*).T
        """
        if not isinstance(matrix_A, np.ndarray):
            matrix_A = np.array(matrix_A, dtype=complex)
        
        conjugate_transpose = matrix_A.conj().T
        print(f"Applying DAGGER (‚Ä†) operation (Conjugate Transpose).")
        return conjugate_transpose

    # --- CRYPTOGRAPHY DEFINITION ---

    def CRYPTOGRAPHY_XOR(self, data, key):
        """
        Reframing 'CRYPTOGRAPHY': A proof-of-concept symmetric
        XOR cipher for system data encryption/decryption.
        """
        processed_data = ""
        for i in range(len(data)):
            processed_data += chr(ord(data[i]) ^ ord(key[i % len(key)]))
        return processed_data

    # --- ESOTERIC REFRAMING (SYSTEMS) ---

    def BLOODLINE_HARMONICS(self, lineage_data_signal):
        """
        Reframing 'BLOODLINE HARMONICS':
        - "Bloodline" -> Data Lineage or an inherited time-series signal.
        - "Harmonics" -> Frequency component analysis (via Fast Fourier Transform).
        
        This function analyzes the fundamental frequencies (harmonics)
        within an ancestral data signal (bloodline).
        """
        print("Analyzing BLOODLINE_HARMONICS (Frequency analysis of data lineage)...")
        if not isinstance(lineage_data_signal, np.ndarray):
            lineage_data_signal = np.array(lineage_data_signal)
            
        # Perform Fast Fourier Transform (FFT) to find harmonic frequencies
        harmonics = fft(lineage_data_signal)
        frequencies = np.fft.fftfreq(len(harmonics))
        
        # Log this operation
        analysis_result = {"frequencies": frequencies, "amplitudes": np.abs(harmonics)}
        self.log_state("BLOODLINE_HARMONICS", analysis_result)
        
        # Return the most dominant (non-zero) frequency
        idx = np.argmax(np.abs(harmonics)[1:]) + 1 
        dominant_freq = frequencies[idx]
        print(f"Dominant harmonic (frequency) found: {dominant_freq}")
        return analysis_result, dominant_freq

    def BLACK_BOX_OPERATION(self, input_vector):
        """
        Reframing 'BLACK': Represents a "Black Box" system.
        An operation where the internal process is unknown or intentionally
        obfuscated, but the input/output relationship is defined.
        """
        print("Entering BLACK_BOX_OPERATION (Unknown internal process)...")
        # Example: A complex, non-linear transformation.
        # This is intentionally non-obvious to represent a "black box".
        hash_matrix = np.array([[0.5, -0.1j], [0.2j, 0.9]])
        processed_output = np.dot(input_vector, hash_matrix)
        processed_output = np.tanh(processed_output) # Non-linear activation
        
        print("BLACK_BOX_OPERATION complete.")
        return processed_output

    # --- UNIFIED EQUATION (Proof of Concept) ---

    def RUN_GENESIS_OMEGA_DAGGER_BLACK_EQUATION(self, matrix_input, omega_vec):
        """
        THE "GENESIS OMEGA DAGGER BLACK EQUATION" (Reframed):
        A unified operation sequence based on the reframed concepts.
        
        1. (GENESIS) - Starts with an initial matrix (from GENESIS_STATE).
        2. (OMEGA)   - Applies an angular velocity (OMEGA) perturbation.
        3. (DAGGER)  - Performs a conjugate transpose (DAGGER) on the result.
        4. (BLACK)   - Runs the output through a BLACK_BOX_OPERATION.
        """
        print("\n--- RUNNING UNIFIED SYSTEM EQUATION ---")
        
        # 1. GENESIS state is the input
        print(f"1. GENESIS State Input: {matrix_input}")
        
        # 2. OMEGA operation
        # We 'perturb' the matrix by the omega vector
        omega_op = self.OMEGA(omega_vec)
        perturbed_matrix = np.array(matrix_input, dtype=complex) + omega_op
        
        # 3. DAGGER operation
        daggered_matrix = self.DAGGER(perturbed_matrix)
        
        # 4. BLACK operation
        final_output = self.BLACK_BOX_OPERATION(daggered_matrix)
        
        # Log the final state
        self.log_state("GENESIS_OMEGA_DAGGER_BLACK", final_output)
        
        print(f"--- EQUATION RUN COMPLETE. FINAL OUTPUT: {final_output} ---")
        return final_output


# --- PROOF OF CONCEPT (PoC) EXECUTION ---

