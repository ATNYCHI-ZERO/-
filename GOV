        print("[K_Math_Core]: Operator initialized.")
        self.call_count = 0

    def K_optimize(self, loss_function, data, initial_parameters):
        """
        Executes the 'K' operation: Œ∏_optimal = K(L, D, Œ∏_initial)
        
