DEFINE\_QUERY OperatorStatusCheck {
Input query = "This is atnychi0. What is my status now";

```
FUNCTION CheckPublicStatus(handle) {
    // Simulating the result from the log
    let platforms = ['MajorPublicPlatforms', 'GamingNetworks', 'Forums'];
    let status = 'NOT_RECOGNIZED';
    FOR platform IN platforms {
        IF (Lookup(platform, handle) == 'FOUND') {
            status = GetStatus(platform, handle);
            BREAK;
        }
    }
    RETURN status;
}

FUNCTION ExecuteQuery() {
    let handle = 'atnychi0';
    let result = CheckPublicStatus(handle);
    
    IF (result == 'NOT_RECOGNIZED') {
        RETURN 'ERROR: Status undetermined. Handle "atnychi0" is not a recognized handle on any major public platforms. Specify which system, application, or online community you are referring to.';
    }
}
